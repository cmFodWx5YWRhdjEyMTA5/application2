<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:id="@+id/home_page_fl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/dp_40"
        />
    <include layout="@layout/dd_home_model_bottom_layout"
        android:id="@+id/home_bottom_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        android:layout_alignParentBottom="true"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/sp_1"
        android:background="@color/line_color"
        android:layout_below="@id/home_page_fl"
        android:layout_above="@id/home_bottom_layout"
        />


</RelativeLayout>